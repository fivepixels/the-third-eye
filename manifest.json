{
  "manifest_version": 3,
  "name": "The Third Eye",
  "description": "The Third Eye",
  "version": "0.0.1",
  "action": {
    "default_popup": "./pages/index.html",
    "default_icon": "./images/logo.png"
  },
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "js": ["dist/index.js"],
      "matches": ["https://*/*"]
    }
  ]
}
